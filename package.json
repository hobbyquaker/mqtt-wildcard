{
  "name": "mqtt-wildcard",
  "version": "3.0.3",
  "description": "Match a MQTT Topic against Wildcards",
  "main": "index.js",
  "scripts": {
    "test": "camo-purge ; xo --space 4 --ignore test.js --ignore benchmark.js && nyc mocha ./test.js && nyc report --reporter=text-lcov | coveralls --force",
